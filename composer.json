{
    "name": "mohammad-fouladgar/Laravel-Filter",
    "description": "",
    "keywords": ["laravel","eloquent","search","filter"],
    "support": {
        "issues": "https://github.com/mohammad-fouladgar/Laravel-Filter/issues",
        "source": "https://github.com/mohammad-fouladgar/Laravel-Filter"
    },
    "authors": [
        {
            "name": "Mohammad Fouladgar",
            "email": "mohammadfouladgarphp@gmail.com",
            "role": "Developer"
        }
    ],
    "license" : "MIT",
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "^5.6",
        "illuminate/container": "^5.6",
        "illuminate/database": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.6",
        "satooshi/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Fouladgar\\Filter\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fouladgar\\Filter\\Test\\":"tests/"
        }
    },
    "extra": {
        "laravel":{
            "providers":[
                "Fouladgar\\Filter\\FilterServiceProvider"
            ]
        }
    },
    "scripts": {
        "test":"phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
